<TcPlcObject Version="1.1.0.1">
  <POU Name="FB_Method3_WithStructuredArguments" Id="{b7161503-0ca3-45a5-91c1-6d74d1d1cfb5}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'OPC.UA.DA.JobMethod' := 'Method3_WithStructuredArguments'}
FUNCTION_BLOCK PUBLIC FB_Method3_WithStructuredArguments
VAR
	// Timer to simulate job execution
	timer : TON := (PT:=T#4S);
	
	// Variables to determine if a job has been started (Start() method has been called) or finished
	jobRunning : BOOL;

	// State machine variable
	state : UDINT;
	
	//Values of method specific input parameters:
	stArgIn : ST_Complex_1;
	
	//Values of method specific output parameters:
	stArgOut : ST_Complex_1;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[CASE state OF
	0:
		IF jobRunning THEN
			state := state + 1;
		END_IF
		
	1:	//Logic of method in here:
		timer(IN:=TRUE);
		IF timer.Q THEN
			
			//Set Inputs To Outputs
			stArgOut := stArgIn;
			
			timer(IN:=FALSE);
			jobRunning := FALSE;
			state := 0;
		END_IF
		
END_CASE]]></ST>
    </Implementation>
    <Method Name="Abort" Id="{b9b4c09f-c269-4d38-b454-1822211ac7b3}">
